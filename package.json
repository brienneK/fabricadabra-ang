{
  "name": "fabricadabra-ang",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=local.dev.com",
    "kill-ports": "npx kill-port 4000 && npx kill-port 4500 && npx kill-port 5000 && npx kill-port 8080 && npx kill-port 9099 && npx kill-port 9150 && npx kill-port 9199",
    "export-data": "rm seed-data -r -f && firebase emulators:export seed-data",
    "local-dev": "firebase emulators:start --import seed-data",
    "get-indexes": "firebase firestore:indexes",
    "build:prod": "npm --no-git-tag-version version patch && node ./setBuildDate.js && ng build --configuration=production",
    "build:ci": "node ./setBuildDate.js && ng build --configuration=production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "update-angular": "ng update @angular/cli @angular/core @angular/material @angular/cdk"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "firebase": "^11.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular/cli": "^17.3.3",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}
